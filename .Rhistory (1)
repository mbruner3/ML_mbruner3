knitr::opts_chunk$set(echo = TRUE)
library(ggplot2)
library(caret)
library(ISLR)
library(readr)
UniBank <- read_csv("UniversalBank.csv")
head(UniBank)
summary(UniBank)
norm_unibank <- preProcess(UniBank, method = c("range"))
default_unibank <- predict(norm_unibank, UniBank)
summary(default_unibank)
Train_Index=createDataPartition(UniBank$,p=0.75, list=FALSE)
dummy_education <- dummyVars(~education, data=UniBank)
dummy_education <- dummyVars(~Education, data=UniBank)
head(predict(dummy_education,UniBank))
View(dummy_education)
View(dummy_education)
